{
  "expo": {
    "name": "YAWT",
    "slug": "yawt",
    "version": "1.0.0",
    "scheme": "yawt",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D0D0D"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yawt.app",
      "deploymentTarget": "16.2",
      "usesIcloudStorage": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "usesAppleSignIn": true,
        "NSSupportsLiveActivities": true,
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0D0D0D"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.yawt.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-notifications",
      [
        "react-native-health",
        {
          "healthSharePermission": "YAWT uses your health data to sync your workout history.",
          "healthUpdatePermission": "YAWT saves your completed workouts to help you close your activity rings."
        }
      ],
      "expo-apple-authentication",
      "expo-secure-store",
      [
        "react-native-cloud-storage",
        {
          "iCloudContainerIdentifier": "iCloud.com.yawt.app"
        }
      ],
      "@react-native-google-signin/google-signin",
      "./plugins/withLiveActivities"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7c088282-1ec2-4b14-bebe-92addc6d864a"
      }
    }
  }
}